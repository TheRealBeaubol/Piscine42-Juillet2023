		x++;
	}
	return (0);
}



#include <stdio.h>
int	main(void)
{
	char	*i;
	char	*j;
	char	**tab;
	int x;

	i = "ttt";
	j = "ttt";
	tab = ft_split(i, j);
	x = 0;
	while(tab[x])
	{
		printf("tab[%d] = %s\n", x, tab[x]);
		x++;
	}
	return (0);
}


int	ft_strlen(char *str)
{
	int	n;

	n = 0;
	while (str[n] != '\0')
		++n;
	return (n);
}

int	main(void)
{
	int	i;
	//char	*str2 = "coucou\n";
