}





#include <stdio.h>
int	mai3n(void)
{
	char	*i;
	char	*j;
	char	**tab;
	int x;

	i = "bienbienbienbien-lolololo";
	j = "bo";
	tab = ft_split(i, j);
	x = 0;
	while(tab[x])
	{
		printf("tab[%d] = %s\n", x, tab[x]);
		x++;
	}
	return (0);
}



