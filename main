int	main(void)
{
	int	i;
	int	j;
	int	*tab;

	i = 20;
	j = 23;
	tab = ft_range(i, j);
	printf("%d %d %d %d", tab[20], tab[21], tab[22], tab[23]);
	i = 0;
	j = 10;
	tab = ft_range(i, j);
	printf("%d %d %d %d %d %d %d %d %d %d %d", tab[0], tab[1], tab[2], tab[3], tab[4], tab[5], tab[6], tab[7], tab[8], tab[9], tab[10]);
}


