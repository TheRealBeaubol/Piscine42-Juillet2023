#include <stdio.h>

int	main()
{
	int	a;
	char	base10[] = "0123456789";
	char	base16[] = "0123456789abcdef";
	char	base2[] = "01";
	char	base8[] = "poneyvif";

	a = -16;
	ft_putnbr_base(a, base10);
	printf("%s","\n");
	ft_putnbr_base(a, base8);
	printf("%s","\n");
	ft_putnbr_base(a, base2);
	printf("%s","\n");
	ft_putnbr_base(a, base16);
}

#include <stdio.h>

int	main()
{
	char	a[] = "    \n  ---++--100";
	char	base10[] = "0123456789";
	char	base16[] = "0123456789abcdef";
	char	base2[] = "01";
	char	base8[] = "poneyvif";

	printf("%d",ft_atoi_base(a, base2));
	printf("%s","\n");
	printf("%d",ft_atoi_base(a, base8));
	printf("%s","\n");
	printf("%d",ft_atoi_base(a, base10));
	printf("%s","\n");
	printf("%d",ft_atoi_base(a, base16));
}

